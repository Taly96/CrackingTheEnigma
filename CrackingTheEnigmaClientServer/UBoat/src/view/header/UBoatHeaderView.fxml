   <children>
      <Button fx:id="buttonLoad" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#onLoadXMLFile" prefHeight="25.0" prefWidth="60.0" text="Load" textAlignment="CENTER" />
      <TextField fx:id="textFieldXMLFile" editable="false" minHeight="-Infinity" prefHeight="25.0" promptText="Please click the Load button to load an XML file." HBox.hgrow="ALWAYS" />
   </children>
</HBox>
