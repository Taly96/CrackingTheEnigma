<VBox alignment="TOP_CENTER" minHeight="-Infinity" minWidth="-Infinity" prefHeight="100.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.bottom.UBoatBottomController">
   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" text="Current Code Configuration" textAlignment="CENTER">
         <font>
            <Font name="Bauhaus 93" size="25.0" />
         </font>
      </Label>
      <TableView fx:id="tableViewCodeConfig" minHeight="-Infinity" minWidth="-Infinity" prefHeight="50.0" prefWidth="600.0">
        <columns>
          <TableColumn editable="false" prefWidth="75.0" sortable="false" text="Rotors" />
          <TableColumn editable="false" prefWidth="75.0" sortable="false" text="Reflector" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Text fx:id="textCodeConfig" strokeType="OUTSIDE" strokeWidth="0.0" text="&lt;None Yet&gt;" textAlignment="CENTER">
         <font>
            <Font size="15.0" />
         </font>
      </Text>
